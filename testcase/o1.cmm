struct Operands
{
	int o1;
	int o2;
};

int add(struct Operands temp)
{
	int t = temp.o1;
	write(temp.o1);
	if(temp.o1 > 100) return temp.o1;
	temp.o1 = temp.o1 + temp.o2;
	temp.o2 = t;
	return add(temp);
	
}

int main()
{
	int n;
	struct Operands op;
	op.o1 = 1;
	op.o2 = 2;
	n = add(op);
	write(op.o1);
	return 0;
}
